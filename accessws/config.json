{
    "debug": true,
    "process": {
        "file_limit": 1000000,
        "core_limit": 1000000000
    },
    "log": {
        "path": "/var/log/trade/accessws",
        "flag": "fatal,error,warn,info,debug,trace"
    },
    "svr": {
        "bind": [
            "stream@/tmp/accessws.sock"
        ],
        "max_pkg_size": 102400,
        "protocol": "chat"
    },
    "monitor": {
        "bind": "tcp@0.0.0.0:8091",
        "max_pkg_size": 1024
    },
    "worker_num": 2,
    "timeout": 1.0,
    "matchengine": {
        "name": "matchengine",
        "addr": [
            "tcp@127.0.0.1:7316"
        ],
        "max_pkg_size": 2000000
    },
    "marketprice": {
        "name": "marketprice",
        "addr": [
            "tcp@127.0.0.1:7416"
        ],
        "max_pkg_size": 2000000
    },
    "readhistory": {
        "name": "readhistory",
        "addr": [
            "tcp@127.0.0.1:7424"
        ],
        "max_pkg_size": 2000000
    },
    "timeout": 1.0,
    "auth_url": "http://192.168.1.6:9001/internal/exchange/user/auth"
}
